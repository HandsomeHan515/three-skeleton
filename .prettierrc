{
	"printWidth": 160,
	"proseWrap": "always",
	"useTabs": false,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"trailingComma": "all",
	"arrowParens": "always",
	"endOfLine": "lf",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"htmlWhitespaceSensitivity": "ignore",
	"jsxSingleQuote": false,
	"rangeStart": 0
}
